{
  "version": 2,
  "builds": [
    {
      "src": "frontend-web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend-web/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://your-backend-url.onrender.com/api"
  }
}
